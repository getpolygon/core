{
  "private": true,
  "scripts": {
    "test": "ava",
    "clean": "rimraf build/",
    "build": "tsc --project .",
    "prestart": "yarn migrate up",
    "prepare": "ts-patch install -s",
    "prebuild": "yarn clean && yarn prepare",
    "migrate": "./bin/goose -dir=migrations",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "express-graph": "express-router-dependency-graph --rootDir=./src/ --format=markdown",
    "dev": "yarn prepare && cross-env NODE_ENV=development nodemon --files ./src/server.ts"
  },
  "dependencies": {
    "@trycourier/courier": "^3.2.0",
    "bcrypt": "^5.0.0",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "deep-email-validator-extended": "^0.2.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "handlebars": "^4.7.7",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.1",
    "redis": "^4.0.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.177",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.11",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.1",
    "@types/redis": "^2.8.31",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "ava": "^3.15.0",
    "errorhandler": "^1.5.1",
    "eslint": "^8.4.0",
    "express-router-dependency-graph": "^1.1.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "ts-patch": "^2.0.1",
    "typescript": "^4.5.2",
    "typescript-transform-paths": "^3.3.1"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}